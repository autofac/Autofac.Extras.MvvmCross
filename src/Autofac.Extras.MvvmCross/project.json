{
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../build/Analyzers.ruleset"
    ],
    "nowarn": [
      "CS1591"
    ],
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "dependencies": {
    "Autofac": "4.0.0-*",
    "Microsoft.CodeAnalysis.FxCopAnalyzers": {
      "type": "build",
      "version": "1.1.0"
    },
    "MvvmCross.Core": "4.0.0",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "System.Runtime.Analyzers": {
      "type": "build",
      "version": "1.1.0"
    }
  },
  "description": "Autofac extension supporting MvvmCross dependency injection.",
  "frameworks": {
    "net451": {
      "dependencies": {
        "System.Reflection": "4.0.0",
        "System.Runtime": "4.0.10"
      }
    },
    "netstandard1.1": {
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.ComponentModel.TypeConverter": "4.1.0",
        "System.Diagnostics.Tools": "4.0.1",
        "System.Globalization": "4.0.11",
        "System.IO": "4.1.0",
        "System.Linq": "4.1.0",
        "System.Reflection.Extensions": "4.0.1",
        "System.Resources.ResourceManager": "4.0.1",
        "System.Runtime": "4.1.0"
      },
      "imports": [
        "portable-net45+win8+wp8+wpa81"
      ]
    }
  },
  "packOptions": {
    "iconUrl": "https://cloud.githubusercontent.com/assets/1156571/13684110/16b8f152-e6bf-11e5-84ae-22c66c6d351a.png",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "owners": [
      "Autofac Contributors"
    ],
    "projectUrl": "http://autofac.org",
    "repository": {
      "type": "git",
      "url": "https://github.com/autofac/Autofac.Extras.MvvmCross"
    },
    "requireLicenseAcceptance": false,
    "summary": "This extension provides support for using Autofac as the DI framework in MvvmCross.",
    "tags": [
      "autofac",
      "di",
      "ioc",
      "dependencyinjection"
    ]
  },
  "version": "4.0.0-*"
}